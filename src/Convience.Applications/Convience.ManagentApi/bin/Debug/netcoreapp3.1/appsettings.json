{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",

      // EF Core的日志输出需要单独配置这行
      // https://stackoverflow.com/questions/56646186/how-to-enable-logging-in-ef-core-3
      // https://docs.microsoft.com/zh-cn/ef/core/miscellaneous/logging?tabs=v3
      // 如果使用的是NET CORE的日志系统，理论上第三方的日志输出都能输出包的日志，但是需要单独配置日志级别
      "Microsoft.EntityFrameworkCore": "Debug"
    }
  },
  "AllowedHosts": "*",

  "ConnectionStrings": {
